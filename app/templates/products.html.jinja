{% extends "layout.html.jinja" %}

{% block middle %}
    <h2>Product List</h2>
    <ul class="list-group">
        {% for product in products %}
            <li class="list-group-item"><a href="{{ url_for('product', product_id=product) }}">{{ product }}</a></li>
        {% endfor %}
    </ul>
{% endblock middle %} 